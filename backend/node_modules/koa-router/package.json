    "path-to-regexp": "1.7"
  },
  "devDependencies": {
    "ava": "^0.25.0",
    "eslint": "^4.19.1",
    "eslint-config-airbnb-base": "12.1.0",
    "eslint-plugin-import": "2.10.0",
    "jsdoc-to-markdown": "^4.0.1",
    "koa": "^2.5.0",
    "mocha": "^5.1.1",
    "nyc": "^11.6.0",
    "supertest": "^3.0.0"
